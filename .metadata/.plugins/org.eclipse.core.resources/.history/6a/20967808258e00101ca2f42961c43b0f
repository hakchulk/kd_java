package study02;

public class StudentTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Student lee = new Student();
		lee.studentName = "이순신";
		lee.address = "서울";
		
		Student han = new Student();
		han.studentName = "한석봉";
		han.address = "부산";
		
		lee.ShowInfo();
		han.ShowInfo();
	}

}
